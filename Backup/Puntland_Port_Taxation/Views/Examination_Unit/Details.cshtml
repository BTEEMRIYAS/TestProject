@model IEnumerable<Puntland_Port_Taxation.Models.Way_BillModel>
<link href="~/Content/Site.css" rel="stylesheet" />
<link href="~/Content/jquery.dataTables.css" rel="stylesheet" />
<script src="~/Scripts/jquery.dataTables.js"></script>
<script>
    $(document).ready(function () {
        $('#tablepaging_way_biil').dataTable();
        $('#tablepaging_way_biil_length').html('');
    });
</script>
<div>
    <a href="javascript:void(0);" onclick="closePopup()"><div class="close_button way_bill_close"></div></a>
    <div class="clear"></div>
</div>
@{
    var i = 0;
        <div class="manage_admin_icon_text_together">
            <div class="manage_admin_icon_text dbsearch_first my_profile_text">Way Bill @TempData["way_bill_code"]</div>
            <div class="clear"></div>
        </div>
        <div class="clear"></div>
        <div class="admninn__pagecontent">
            <div class="jq_table">
                <table id="tablepaging_way_biil">
                        <thead>
                            <tr>
                                <th>
                                    Sl No
                                </th>
                                <th>
                                    Goods
                                </th>
                                <th>
                                    Units
                                </th>
                                <th>
                                    Quantity
                                </th>
                                <th>
                                    Unit Of Measure
                                </th>
                                <th>
                                    Total Quantity
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                i++;
                                    <tr>
                                        <td class="left_border">
                                            @i
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.goods)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.units)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.quantity)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.unit_of_measure)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.total_quantity)
                                        </td>
                                    </tr>
                            }
                        </tbody>
                </table>
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
        </div>
        <div class="clear"></div>
}
