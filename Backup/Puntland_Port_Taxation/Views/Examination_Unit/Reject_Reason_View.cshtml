@model IEnumerable<Puntland_Port_Taxation.Models.Reject_ReasonModel>
<div>
    <a href="javascript:void(0);" onclick="closePopup()"><div class="close_button"></div></a>
    <div class="clear"></div>
</div>
    <div class="form">
        @if(@Model.Count()>0)
        {
            <div class="print_field">
                @Html.Label("Way Bill Code")
                <div class="print_display">
                    @Model.First().way_bill_code
                    <div class="clear"></div>
                </div>                        
                <div class="clear"></div>
            </div>
            <div class="print_field">
                @Html.Label("Reason")
                <div class="print_display">
                    @Model.First().reason
                    <div class="clear"></div>
                </div>                        
                <div class="clear"></div>
            </div> 
            <div class="print_field">
                @Html.Label("Checked Date")
                <div class="print_display">
                    @Model.First().rejected_date.ToString("yyyy-MM-dd")
                    <div class="clear"></div>
                </div>                        
                <div class="clear"></div>
            </div>
            <div class="print_field">
                @Html.Label("Checked By")
                <div class="print_display">
                    @Model.First().rechecked_by
                    <div class="clear"></div>
                </div>                        
                <div class="clear"></div>
            </div>                                  
        }
        <div class="clear"></div>
    </div>