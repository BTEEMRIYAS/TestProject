@model IEnumerable<Puntland_Port_Taxation.Models.ImporterModel>
@{
    Layout = "~/Views/Shared/Shared_Section_Bottom.cshtml";
}
@{
    var i = 0;       
        <div class="manage_admin_icon_text_together">
            <div class="manage_importer_icon"></div>
            <div class="manage_admin_icon_text">Manage Importer</div>
            <div class="clear"></div>
        </div>
        if (TempData["errorMessage"] != null)
        {
            @Html.Hidden("error", TempData["errorMessage"])
        }
        <div class="clear"></div>
        <div class="admninn__pagecontent">
            <div class="jq_table">
                <table id="tablepaging">
                        <thead>
                            <tr>
                                <th>
                                    Sl No
                                </th>
                                <th>
                                    Name
                                </th>
                                <th>
                                    Country
                                </th>
                                <th>
                                    Importer Type
                                </th>
                                <th>
                                    Email
                                </th>
                                <th>
                                    Mobile
                                </th>
                                <th>
                                    Status
                                </th>
                                <th>
                                    Edit
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                    i++;
                                    <tr>
                                        <td class="left_border">
                                            @i
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.importer_first_name) @Html.DisplayFor(modelItem => item.importer_middle_name) @Html.DisplayFor(modelItem => item.importer_last_name)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.importer_country_name)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.importer_type_name)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.importer_email_id)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.importer_mob_no)
                                        </td>
                                        <td>
                                            @if(item.status_id == 1)
                                            {
                                                <div class="active">Active</div>
                                            }
                                            else if(item.status_id == 2)
                                            {
                                                <div class="active deactive">Deactive</div>
                                            }
                                            else if(item.status_id == 3)
                                            {
                                                <div class="active suspended">Suspended</div>
                                            }
                                        </td>
                                        <td>
                                            <a href="javascript:void(0);" onclick="addpopup_edit(@item.importer_id)"><div class="edit_icon"></div></a>
                                        </td>
                                    </tr>
                            }
                        </tbody>
                </table>
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
        </div>
        <div class="clear"></div>
}
