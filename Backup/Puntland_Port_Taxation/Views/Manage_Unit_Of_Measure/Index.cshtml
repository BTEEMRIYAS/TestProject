@model IEnumerable<Puntland_Port_Taxation.Unit_Of_Measure>
@{
    Layout = "~/Views/Shared/Shared_Section_Bottom.cshtml";
}
@{
    var i=0;   
        <div class="manage_admin_icon_text_together">
            <div class="unit_of_measure_icon"></div>
            <div class="manage_admin_icon_text">Unit Of Measure</div>
            <div class="clear"></div>
        </div>
        if (TempData["errorMessage"] != null)
        {
            @Html.Hidden("error", TempData["errorMessage"])
        }
        <div class="clear"></div>
        <div class="admninn__pagecontent">
            <div class="jq_table">
                <table id="tablepaging">
                        <thead>
                            <tr>
                                <th>
                                    Sl No
                                </th>
                                <th>
                                   Unit Code
                                </th>
                                <th>
                                   Unit Name
                                </th>                              
                                <th>
                                    Edit
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                i++;
                                    <tr>
                                        <td class="left_border">
                                            @i
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.unit_code)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.unit_name)
                                        </td>                                     
                                      
                                        <td>
                                            <a href="javascript:void(0);" onclick="addpopup_edit(@item.unit_id)"><div class="edit_icon"></div></a>
                                        </td>
                                    </tr>
                            }
                        </tbody>
                </table>
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
        </div>
        <div class="clear"></div>
}

