@model Puntland_Port_Taxation.Importer_Type
<div>
    <a href="javascript:void(0);" onclick="closePopup()"><div class="close_button"></div></a>
    <div class="clear"></div>
</div>
<div class="form">
    @using (Html.BeginForm(null, null, FormMethod.Post, new { id = "importer_type", name = "importer_type" }))
    {
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true)

            <div class="editor-label">
                @Html.LabelFor(model => model.importer_type_name, "Importer Type")
                @Html.TextBoxFor(model => model.importer_type_name, new { @class = "input" })
                @Html.ValidationMessageFor(model => model.importer_type_name)
                <div id="error_importer_type_name" class="val__msgbx"></div>
                <div class="clear"></div>
            </div>
        
            <div class="editor-label">
                @Html.LabelFor(model => model.is_multiple_allowed,"Multiple Way Bill")
                <div class="percentage_radio">
                    <div class="editor-label_upload_photo">
                        @Html.RadioButtonFor(model => model.is_multiple_allowed,true,new { id = "is_multiple_allowed" }) Yes
                    </div>                   
                    <div class="upload_photo_text">
                        @Html.RadioButtonFor(model => model.is_multiple_allowed,false,new { @checked = true, id = "is_multiple_allowed" }) No
                    </div>
                    <div class="clear"></div>
                </div>
                @Html.ValidationMessageFor(model => model.is_multiple_allowed)
                <div class="clear"></div>
                <div id="error_is_multiple_allowed" class="val__msgbx"></div>
                <div class="clear"></div>
            </div>        

            <div class="editor-label label_button">
                <input type="submit" value="Save" class="save_button" onclick="return importer_type_add()" />
                <a href="javascript:void(0);" class="text_style" onclick="closePopup()">
                    <input type="button" value="Cancel" class="save_button cancel_button" id="createUser" name="Save" />
                </a>
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
    }
    <div class="clear"></div>
</div>
