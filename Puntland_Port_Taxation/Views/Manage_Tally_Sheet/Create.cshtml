@model Puntland_Port_Taxation.Tally_Sheet
<div>
    <a href="javascript:void(0);" onclick="closePopup()"><div class="close_button"></div></a>
    <div class="clear"></div>
</div>
<div class="form">
    @using (Html.BeginForm(null, null, FormMethod.Post, new { id = "tally_sheet", name = "tally_sheet" })) {
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true)  

            <div class="editor-label">
                @Html.LabelFor(model => model.ship_arrival_id, "Ship Arrival Code")
                @Html.DropDownList("ship_arrival_id", (SelectList)ViewBag.ship_arrivals, "Select Ship Arrival", new { @class = "inputselect select" })
                @Html.ValidationMessageFor(model => model.ship_arrival_id)
                <div id="error_ship_arrival_id" class="val__msgbx"></div>
                <div class="clear"></div>
            </div>
                   
            <div class="editor-label label_button">
                <input type="submit" value="Save" class="save_button" onclick="return tally_sheet_add()" />
                <a href="javascript:void(0);" class="text_style" onclick="closePopup()">
                    <input type="button" value="Cancel" class="save_button cancel_button" id="createUser" name="Save" />
                </a>
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
    }
    <div class="clear"></div>
</div>

