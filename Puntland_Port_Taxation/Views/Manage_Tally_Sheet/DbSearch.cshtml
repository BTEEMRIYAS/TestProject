@model Puntland_Port_Taxation.Models.Tally_SheetModel
<div>
    <a href="javascript:void(0);" onclick="closePopup()"><div class="close_button"></div></a>
    <div class="clear"></div>
</div>
<div class="form">
    @using (Html.BeginForm("DbSearchresult", "Manage_Tally_Sheet", FormMethod.Post, new { id = "tally_sheet", name = "tally_sheet" }))
    {
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true)

            <div class="editor-label">
                @Html.LabelFor(model => model.tally_sheet_code, "Cargo Manifest Code")
                @Html.TextBoxFor(model => model.tally_sheet_code, new { @class = "input" })
                <div class="clear"></div>
            </div>        

            <div class="editor-label">
                @Html.LabelFor(model => model.ship_arrival_code, "Ship Arrival Code")
                @Html.TextBoxFor(model => model.ship_arrival_code, new { @class = "input" })
                <div class="clear"></div>
            </div>
                      
            <div class="editor-label label_button">
                <input type="submit" value="Search" class="save_button" />
                <a href="javascript:void(0);" class="text_style" onclick="closePopup()">
                    <input type="button" value="Cancel" class="save_button cancel_button" id="createUser" name="Save" />
                </a>
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
    }
    <div class="clear"></div>
</div>