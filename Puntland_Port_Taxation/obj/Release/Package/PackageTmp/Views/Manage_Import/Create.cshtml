@model Puntland_Port_Taxation.Import
<link href="~/Dropdown_Search/chosen.min.css" rel="stylesheet" />
<script src="~/Dropdown_Search/jquery-2.0.3.min.js"></script>
<script src="~/Dropdown_Search/chosen.jquery.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $(".chosen-select").chosen()
    });
</script>
<div>
    <a href="javascript:void(0);" onclick="closePopup()"><div class="close_button"></div></a>
    <div class="clear"></div>
</div>
<div class="form">
    @using (Html.BeginForm(null, null, FormMethod.Post, new { id = "import", name = "import" }))
    {
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true)

            <div class="editor-label">
                @Html.LabelFor(model => model.importer_id, "Importer Name")
                @Html.DropDownList("importer_id", (SelectList)ViewBag.importer, "Select Importer", new { @class = "inputselect select chosen-select" })
                @Html.ValidationMessageFor(model => model.importer_id)
                <div id="error_importer_id" class="val__msgbx"></div>
                <div class="clear"></div>
            </div>

            <div class="editor-label">
                @Html.LabelFor(model => model.ship_arrival_id, "Ship Arrival Code")
                @Html.DropDownList("ship_arrival_id", (SelectList)ViewBag.ship_arrival, "Select Ship Arrival Code", new { @class = "inputselect select chosen-select" })
                @Html.ValidationMessageFor(model => model.ship_arrival_id)
                <div id="error_ship_arrival_id" class="val__msgbx"></div>
                <div class="clear"></div>
            </div>
        
            <div class="editor-label label_button">
                <input type="submit" id="submit" value="Save" class="save_button" onclick="return import_add()" />
                <a href="javascript:void(0);" class="text_style" onclick="closePopup()">
                    <input type="button" value="Cancel" class="save_button cancel_button" id="createUser" name="Save" />
                </a>
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
    }
    <div class="clear"></div>
</div>
