@model Puntland_Port_Taxation.Models.ImportModel
<div>
    <a href="javascript:void(0);" onclick="closePopup()"><div class="close_button"></div></a>
    <div class="clear"></div>
</div>
<div class="form">
    @using (Html.BeginForm("DbSearchresult", "Manage_Import", FormMethod.Post, new { id = "import", name = "import" }))
    {
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true)

            <div class="editor-label">
                @Html.LabelFor(model => model.importer_name, "Importer Name")
                @Html.TextBoxFor(model => model.importer_name, new { @class = "input" })
                <div class="clear"></div>
            </div>

            <div class="editor-label">
                @Html.LabelFor(model => model.ship_arrival_code, "Ship Arrival Code")
                @Html.TextBoxFor(model => model.ship_arrival_code, new { @class = "input" })
                <div class="clear"></div>
            </div>
        
            <div class="editor-label">
                @Html.LabelFor(model => model.payment_id, "Payment Status")
                @Html.DropDownList("payment_id", new List<SelectListItem> { new SelectListItem{ Text="Not Paid", Value="1"},
                                                                            new SelectListItem{ Text="Paid", Value="2"}
                                                                          }, "Select Payment Status", new { @class = "inputselect select" })
                @Html.ValidationMessageFor(model => model.payment_id)
                <div class="clear"></div>
            </div>

            <div class="editor-label label_button">
                <input type="submit" value="Search" class="save_button" />
                <a href="javascript:void(0);" class="text_style" onclick="closePopup()">
                    <input type="button" value="Cancel" class="save_button cancel_button" id="createUser" name="Save" />
                </a>
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
    }
    <div class="clear"></div>
</div>
