@model IEnumerable<Puntland_Port_Taxation.Models.Way_BillModel>
<script>
    $(document).ready(function () {
        $('#tablepaging').dataTable();
        $('#tablepaging_length').html('&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<a href="javascript:void(0);" onclick="addpopup()" class="dbsearch" ><div class="add_admin">Add</div></a>&nbsp;<a href="javascript:void(0);" onclick="addpopup_search()" class="dbsearch" ><div class="dbsearchbutton">Search</div></a>');
    });
</script>
@{
    Layout = "~/Views/Shared/Shared_Section_Bottom.cshtml";
}
@{
    var i = 0;
    if(ViewBag.start_from != null)
    {
        i = ViewBag.start_from;
    }
    var l = ViewBag.page;
    int count = 0;
    double no_of_pages = 0;
    if (ViewBag.total_page != null)
    {
        count = ViewBag.total_page;
    }
    double c = (double)count / 9;
    no_of_pages = Math.Ceiling(c);
        <div class="manage_admin_icon_text_together">
            <div class="manage_way_bill_icon"></div>
            <div class="manage_admin_icon_text">Declaration Form Import</div>
            <div class="clear"></div>
        </div>
        if (TempData["errorMessage"] != null)
        {
            @Html.Hidden("error", TempData["errorMessage"])
        }
        <div class="clear"></div>
        <div class="admninn__pagecontent">
            <div class="jq_table">
                <table id="tablepaging">
                        <thead>
                            <tr>
                                <th>
                                    Sl No
                                </th>
                                <th>
                                    Way Bill Code
                                </th>
                                <th>
                                    Import Code
                                </th>
                                <th>
                                    Mark
                                </th>
                                <th>
                                    Ship Arrival Code
                                </th>
                                <th>
                                    Add Goods
                                </th>
                                <th>
                                    View
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                i++;
                                    <tr>
                                        <td class="left_border">
                                            @i
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.way_bill_code)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.import_name)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.mark
                                            )
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ship_arrival_code)
                                        </td>
                                        @if (item.import_status_id == 1)
                                        {
                                            <td>
                                                <a class="dbsearch" href="@Url.Action("Details", "Manage_Way_Bill", new { way_bill_id = item.way_bill_id })"><div class="add_admin new_add">Add Goods</div></a>
                                            </td>
                                        }
                                        else if (item.import_status_id == 17)
                                        {
                                            <td>
                                                <a href="javascript:void(0);" class="dbsearch" onclick="view_reject_reason(@item.way_bill_id)"><div class="add_admin new_add reject_add">Rejected</div></a>
                                            </td>                                            
                                        }
                                        else if (item.import_status_id == 14)
                                        {
                                            <td>
                                                Way Bill Resubmitted
                                            </td>
                                        }
                                        else
                                        {
                                            <td>
                                                Way Bill Submitted
                                            </td>
                                        }
                                        <td>
                                            <a href="javascript:void(0);" class="dbsearch" onclick="waybill_view(@item.way_bill_id)"><div class="add_admin calculate_tax">View</div></a>
                                        </td>
                                    </tr>
                            }
                        </tbody>
                </table>
                @if(count > 9)
                {
                    <div class="page_links">
                        <a href="../Manage_Way_Bill?page=1" class="link_class"><div class="left_link_image"></div></a>
                        <a href="../Manage_Way_Bill?page=@(l-1)" class="link_class"><div class="toggle_left_link_image"></div></a>
                        <a href="../Manage_Way_Bill?page=@(l+1)" class="link_class"><div class="toggle_right_link_image"></div></a>
                        <a href="../Manage_Way_Bill?page=@no_of_pages" class="link_class"><div class="right_link_image"></div></a>
                        <div class="clear"></div>
                    </div>
                }
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
        </div>
        <div class="clear"></div>
}
