@model Puntland_Port_Taxation.E_Way_Bill
<div>
    <a href="javascript:void(0);" onclick="closePopup()"><div class="close_button"></div></a>
    <div class="clear"></div>
</div>
<div class="form">
    @using (Html.BeginForm(null, null, FormMethod.Post, new { id = "way_bill", name = "way_bill" }))
    {
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true)

            <div class="editor-label">
                @Html.LabelFor(model => model.e_way_bill_code, "Way Bill Code")
                @Html.TextBoxFor(model => model.e_way_bill_code, new { @class = "input" })
                @Html.ValidationMessageFor(model => model.e_way_bill_code)
                <div id="error_e_way_bill_code" class="val__msgbx"></div>
                <div class="clear"></div>
            </div>

            <div class="editor-label">
                @Html.LabelFor(model => model.export_id, "Export")
                @Html.DropDownList("export_id", (SelectList)ViewBag.export, "Select Export", new { @class = "inputselect select" })
                @Html.ValidationMessageFor(model => model.export_id)
                <div id="error_export_id" class="val__msgbx"></div>
                <div class="clear"></div>
            </div>
        
            <div class="editor-label">
                @Html.LabelFor(model => model.mark, "Mark")
                @Html.TextBoxFor(model => model.mark, new { @class = "input" })
                @Html.ValidationMessageFor(model => model.mark)
                <div id="error_mark" class="val__msgbx"></div>
                <div class="clear"></div>
            </div>

            <div class="editor-label label_button">
                <input type="submit" value="Save" class="save_button" onclick="return e_way_bill_add()" />
                <a href="javascript:void(0);" class="text_style" onclick="closePopup()">
                    <input type="button" value="Cancel" class="save_button cancel_button" id="createUser" name="Save" />
                </a>
                <div class="clear"></div>
            </div>
            <div class="clear"></div>
    }
    <div class="clear"></div>
</div>
